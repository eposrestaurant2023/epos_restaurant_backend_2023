    <DataSet Name="Product">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@product_category_id">
            <Value>=Parameters!product_category_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_group_id">
            <Value>=Parameters!product_group_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@option_id">
            <Value>=Parameters!option_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stock_location_id">
            <Value>=Parameters!stock_location_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>			  
				  		
if object_id('tempdb..#tbl') is not null drop table #tbl
select 
	p.id,
	p.product_category_en, 
	p.product_category_kh,
	p.product_code,
	p.product_name_en,
	p.product_name_kh,
	p.cost,
	p.min_price,
	p.max_price,
	p.unit_id,
	u.unit_name,
	p.quantity,
	p.is_inventory_product
	into #tbl
from tbl_product p 
	inner join tbl_product_category pc on pc.id = p.product_category_id
	inner join tbl_unit u on u.id = p.unit_id
where 
	p.is_deleted = 0 and 
	p.status = 1 and 
	p.is_inventory_product = 1 and 
	p.product_category_id = iif(@product_category_id = 0, p.product_category_id, @product_category_id) and 
	pc.product_group_id= iif(@product_group_id= 0, p.product_group_id, @product_group_id) 





-- update quantity by business branch
if object_id('tempdb..#tbl_inventory') is not null drop table #tbl_inventory
select 
	a.product_id,
	sum(a.quantity) as quantity
	into #tbl_inventory
from tbl_stock_location_product a 
inner join #tbl t on t.id = a.product_id
inner join tbl_stock_location b on b.id = a.stock_location_id
inner join dbo.fn_get_business_branch_by_role(@printed_by) c on c.id = b.business_branch_id
where 
	c.id &lt;&gt; '' and 
	cast(b.business_branch_id as nvarchar(50)) = iif(@business_branch_id = '', cast(b.business_branch_id as nvarchar(50)),@business_branch_id) and 
	cast(a.stock_location_id as nvarchar(50)) = iif(@stock_location_id = '', cast(a.stock_location_id as nvarchar(50)),@stock_location_id)
group by
	a.product_id

---update quantity to product
if @business_branch_id &lt;&gt; '' or @stock_location_id &lt;&gt; ''
begin
	update #tbl set quantity = 0 
end
update t 
	set t.quantity = v.quantity
from #tbl t inner join #tbl_inventory v on v.product_id = t.id 


--check if product is none stock and if quantity &gt; 0 update to  0
update #tbl set quantity = 0 where is_inventory_product = 0; 


--3 option_id = 3 show product have stock
if @option_id = 3 delete #tbl where  quantity&lt;=0;
--4 option_id = 4 show product out of stock
if @option_id = 4 delete #tbl where quantity&gt;0;


  select * from #tbl order by product_category_en, product_name_en</CommandText>
