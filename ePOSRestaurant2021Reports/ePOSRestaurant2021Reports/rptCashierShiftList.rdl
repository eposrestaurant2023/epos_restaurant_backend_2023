    <DataSet Name="CashiftShiftData">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@outlet_id">
            <Value>=Parameters!outlet_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> 
 
if object_id('tempdb..#tbl') is not null drop table #tbl
select 
	a.id,
	a.cashier_shift_number,
	w.working_date,
	w.working_day_number,
	w.business_branch_id,
	b.business_branch_name_en,
	a.outlet_name_en,
	a.opened_station_name_en,
	a.closed_station_name_en,
	a.created_by,
	a.created_date,
	a.closed_by,
	a.closed_date,
	a.open_amount,
	a.close_amount,
	iif(a.is_closed=0,'Opened','Closed') as [status],
	0 as total_sale_order,
	cast(0 as decimal(16,4)) as difference_amount,
	cast(0 as decimal(16,4)) as sub_total,
	cast(0 as decimal(16,4)) as total_sale_amount,
	cast(0 as decimal(16,4)) as total_discount_amount,
	cast(0 as decimal(16,4)) as total_tax_amount

	into #tbl
from tbl_cashier_shift  a
	inner join tbl_working_day w on w.id = a.working_day_id
	inner join dbo.fn_get_business_branch_by_role(@printed_by) b on b.id = w.business_branch_id
where
	cast(w.business_branch_id as nvarchar(50))  = iif(@business_branch_id= '',cast(w.business_branch_id as nvarchar(50)),@business_branch_id) and 
	cast(a.outlet_id as nvarchar(50))  = iif(@outlet_id= '',cast(a.outlet_id as nvarchar(50)),@outlet_id) and 
	a.is_deleted = 0 and
	a.working_date between @start_date and @end_date
order by
	a.cashier_shift_number 

 
--get total sale 
if object_id('tempdb..#tbl_sale') is not null drop table #tbl_sale
select 
	s.closed_cashier_shift_id,
	count(s.id) as total_sale_order,
	sum(s.sub_total) as sub_total,
	sum(s.total_amount + s.coupon_voucher_amount) as total_amount,
	sum( s.total_discount_amount) as total_discount,
	sum(s.total_tax_amount) as total_tax_amount
	into #tbl_sale
from tbl_sale s 
inner join #tbl t on t.id = s.closed_cashier_shift_id
where
	 s.status_id in (4)
group by
	s.closed_cashier_shift_id

update t
	set t.total_sale_order = s.total_sale_order,
		t.total_discount_amount = s.total_discount,
		t.total_tax_amount = s.total_tax_amount,
		t.total_sale_amount = s.total_amount,
		t.sub_total = s.sub_total 
from #tbl t 
	inner join #tbl_sale s on s.closed_cashier_shift_id = t.id 


--get differ
 

;with a as(
	select 
		a.cashier_shift_id,
		sum(a.base_currency_amount) as amount
	from tbl_cash_drawer_amount a
	inner join #tbl t on t.id = a.cashier_shift_id
	where
		a.transaction_type_name = 'Differential Amount'
	group by
		a.cashier_shift_id
)
update t set t.difference_amount = a.amount  from #tbl t inner join a on a.cashier_shift_id = t.id

select 
	*
from #tbl </CommandText>
