    <DataSet Name="Product">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_category_id">
            <Value>=Parameters!product_category_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_group_id">
            <Value>=Parameters!product_group_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
--GET PRODUCT ID ONLY FOR FILTER BY BRANCH

if object_id('tempdb..#tbl_product') is not null drop table #tbl_product;
select 
	distinct product_id 
	into #tbl_product 
from tbl_product_menu pm
	inner join tbl_menu m on m.id = pm.menu_id
	inner join	dbo.fn_get_business_branch_by_role(@printed_by) b on b.id = m.business_branch_id
where 
	cast(m.business_branch_id as nvarchar(50)) = iif(@business_branch_id='', cast(m.business_branch_id as nvarchar(50)),@business_branch_id) 
--GET PRODUCT WITH MORE DETAIL INFORMATION 
  
	 

select 
	distinct
	p.product_category_id,
	p.product_category_en


from tbl_product p 
	inner join #tbl_product bp on bp.product_id = p.id
where
	isnull(p.product_code,'') &lt;&gt; '' and
	p.is_deleted = 0 and 
	p.status = 1 and 
	p.is_menu_product = 1 and 
	p.product_category_id = iif(@product_category_id = 0, p.product_category_id, @product_category_id) and 
	p.product_group_id= iif(@product_group_id= 0, p.product_group_id, @product_group_id) 
	 </CommandText>
      </Query>
