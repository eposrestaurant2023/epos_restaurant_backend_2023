        <Field Name="product_category_id">
          <DataField>product_category_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="product_category_en">
          <DataField>product_category_en</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
