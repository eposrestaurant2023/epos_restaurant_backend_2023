    <DataSet Name="SalePayment">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@outlet_id">
            <Value>=Parameters!outlet_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customer_id">
            <Value>=Parameters!customer_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customer_group_id">
            <Value>=Parameters!customer_group_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select 
	s.working_date,
	p.payment_type_name_en,
	p.currency_format,
	sum(s.total_amount) as sale_amount,
	sum(p.payment_amount) as payment_amount,
	sum(p.payment_amount/p.exchange_rate) as base_payment_amount
from tbl_sale_payment p
inner join tbl_sale s on s.id = p.sale_id
left join tbl_customer c on c.id = s.customer_id 
where
	s.is_deleted = 0 and 
	s.working_date between @start_date and @end_date and 
	cast(s.business_branch_id as nvarchar(50)) = iif(@business_branch_id='', cast(s.business_branch_id as nvarchar(50)),@business_branch_id) and 
	cast(s.outlet_id as nvarchar(50)) = iif(@outlet_id='', cast(s.outlet_id as nvarchar(50)),@outlet_id) and 
	s.is_foc = 0 and 
	coalesce(cast(c.id as nvarchar(50)),'') = iif(@customer_id = '', coalesce(cast(c.id as nvarchar(50)),'') ,@customer_id) and 
	coalesce(c.customer_group_id,0) = iif(@customer_group_id= 0,coalesce(c.customer_group_id,0),@customer_group_id) 
group by
	s.working_date,
	p.payment_type_name_en,
	p.currency_format
</CommandText>
