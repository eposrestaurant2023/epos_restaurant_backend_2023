    <DataSet Name="PurchaseOrder">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stock_location_id">
            <Value>=Parameters!stock_location_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@vendor_id">
            <Value>=Parameters!vendor_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@vendor_group_id">
            <Value>=Parameters!vendor_group_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_category_id">
            <Value>=Parameters!product_category_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_group_id">
            <Value>=Parameters!product_group_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
 select 
	p.product_name_en,
	p.product_category_en,
	avg(pop.cost) as cost,
	SUM(pop.quantity) as quantity,
	SUM(pop.total_discount) as total_discount,
	SUM(pop.sub_total) as sub_total,
	SUM(pop.total_amount) as total_amount
from tbl_purchase_order_product pop
	inner join tbl_purchase_order po on po.id = pop.purchase_order_id
	inner join	dbo.fn_get_business_branch_by_role(@printed_by) b on b.id = po.business_branch_id
	inner join tbl_vendor v on  v.id = po.vendor_id
	inner join tbl_product p on p.id = pop.product_id 
where 
	po.purchase_date between @start_date and @end_date and
	cast(po.business_branch_id  as nvarchar(50)) = iif(@business_branch_id='',cast(po.business_branch_id as nvarchar(50)),@business_branch_id) and
	cast(po.stock_location_id  as nvarchar(50)) = iif(@stock_location_id='',cast(po.stock_location_id as nvarchar(50)),@stock_location_id) and 
	po.is_deleted = 0 and 
	pop.is_deleted = 0 and 
	po.vendor_id = iif(@vendor_id = 0,po.vendor_id,@vendor_id) and 
	v.vendor_group_id  = iif(@vendor_group_id = 0,v.vendor_group_id,@vendor_group_id) and 
	p.product_category_id = iif(@product_category_id = 0,p.product_category_id,@product_category_id) and 
	p.product_group_id = iif(@product_group_id = 0,p.product_group_id,@product_group_id)
group by
	p.product_category_en,
	p.product_name_en</CommandText>
      </Query>
