    <DataSet Name="PurchaseOrderProduct">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	a.cost,
	a.discount,
	a.discount_type,
	a.quantity,
	a.total_amount,
	b.product_name_en,
	a.unit
	
from 
	tbl_purchase_order_product a
	inner join tbl_product b on a.product_id = b.id
where 
	purchase_order_id = @id and 
	a.is_deleted = 0</CommandText>
      </Query>
