    <DataSet Name="PurchaseOrder">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stock_location_id">
            <Value>=Parameters!stock_location_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@vendor_id">
            <Value>=Parameters!vendor_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	b.business_branch_name_en,
	a.purchase_date,
	a.document_number,
	a.discount,
	a.discount_type,
	a.total_amount,
	a.balance,
	a.paid_amount,
	a.total_discount,
	c.vendor_code,
	c.vendor_name,
	c.office_phone,
	d.stock_location_name
from 
	tbl_purchase_order a
	inner join fn_get_business_branch_by_role(@printed_by) b on a.business_branch_id = b.id
	inner join tbl_vendor c on a.vendor_id = c.id 
	inner join tbl_stock_location d on d.id = a.stock_location_id
where 
	a.id = @id and
	a.purchase_date between @start_date and @end_date and
	a.business_branch_id = iif(@business_branch_id='',a.business_branch_id,@business_branch_id) and
	a.stock_location_id = iif(@stock_location_id = '',a.stock_location_id,@stock_location_id) and
	a.vendor_id = iif(@vendor_id=0,a.vendor_id,@vendor_id)</CommandText>
      </Query>
      <Fields>
        <Field Name="business_branch_name_en">
          <DataField>business_branch_name_en</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="purchase_date">
