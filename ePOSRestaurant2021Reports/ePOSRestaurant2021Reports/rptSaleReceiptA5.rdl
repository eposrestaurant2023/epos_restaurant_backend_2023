          </QueryParameter>
        </QueryParameters>
        <CommandText>	select 
	a.document_number,
	a.sale_number,
	a.sale_discount_type,
	a.sale_discount_value,
	a.guest_cover,
	a.closed_date,
	a.created_date,
	a.sale_type,
	c.business_branch_name_en,
	c.address_en,
	c.phone_1,
	c.phone_2,
	a.outlet_name_en,
	a.paid_amount,
	a.balance,
	a.total_discount_amount,
	a.working_day_number,
	a.cashier_shift_number, 
	a.sub_total,
	a.total_quantity,
	a.closed_by,
	a.customer_code,
	a.customer_name,
	a.phone_number ,
	a.table_name,
	a.station_name_en,
	a.tax_1_rate,
	a.tax_1_amount,
	a.tax_2_rate,
	a.tax_2_amount,
	a.tax_3_rate,
	a.tax_3_amount ,
	a.total_amount
from tbl_sale a 
inner join tbl_business_branch c on c.id = a.business_branch_id

where
cast(a.id as nvarchar(50)) = @id</CommandText>
