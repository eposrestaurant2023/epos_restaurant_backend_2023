    <DataSet Name="PaymentData">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare 
		@total_amount decimal(16,4),
		@payment_receive_amount decimal(16,4);
	select 
		@total_amount = total_amount
	from tbl_sale
	where
		id = @id

  
	select 
		@payment_receive_amount =  sum(payment_amount/exchange_rate) 
	from tbl_sale_payment 
	where 
		sale_id = @id and 
		is_deleted = 0;

		select 
			payment_type_name_en,
			payment_type_name_kh,
			currency_format as [format],
			@payment_receive_amount - @total_amount as change_amount,
			sum(payment_amount) as payment_amount
		from tbl_sale_payment where 
		sale_id = @id and 
		is_deleted = 0
		group by
			payment_type_name_en,
			payment_type_name_kh,
			currency_format</CommandText>
      </Query>
      <Fields>
        <Field Name="payment_type_name_en">
          <DataField>payment_type_name_en</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="payment_type_name_kh">
          <DataField>payment_type_name_kh</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="format">
          <DataField>format</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="change_amount">
          <DataField>change_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="payment_amount">
          <DataField>payment_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
