    <DataSet Name="sale_payment">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	select  
	b.payment_type_name_en,
	a.payment_amount,
	c.report_format
	from tbl_sale_payment a
	inner join tbl_payment_type b on a.payment_type_id = b.id
	inner join tbl_currency c on c.id = a.currency_id
	where 
	a.is_deleted = 0 and a.status = 1 and
	cast(sale_id As nvarchar(50))= @id</CommandText>
      </Query>
