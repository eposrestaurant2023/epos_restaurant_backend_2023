    <DataSet Name="sale_data">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sale_id">
            <Value>=Parameters!sale_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
a.document_number,
a.working_date,
c.business_branch_name_en,
b.outlet_name_en,
a.paid_amount,
a.balance,
a.total_discount_amount
from tbl_sale a 
inner join tbl_outlet b on a.outlet_id = b.id
inner join tbl_business_branch c on c.id = a.business_branch_id
where 
	a.id = @sale_id</CommandText>
      </Query>
      <Fields>
        <Field Name="document_number">
