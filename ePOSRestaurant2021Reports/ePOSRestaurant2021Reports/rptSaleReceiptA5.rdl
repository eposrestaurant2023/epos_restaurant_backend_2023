  <DataSets>
    <DataSet Name="sale_product">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sale_id">
            <Value>=Parameters!sale_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	note,
	product_name_en,
	product_name_kh,
total_discount_amount,
	quantity,
	price,
	total_amount
from tbl_sale_product
where 
	sale_id = @sale_id and
	is_deleted = 0 and
	status = 1</CommandText>
      </Query>
