    <DataSet Name="GrandTotalData">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare 
		@currency nvarchar(max),
		@total_amount decimal(16,4);
			 
	  
	select 
		@total_amount = total_amount,
		@currency = currency_exchange_rate_data
	from tbl_sale
	where
		id = @id 

 --get currency for Grand Total Amount Data
	 if object_id('tempdb..#tbl_currency') is not null drop table #tbl_currency
	SELECT *,  
		'name en' as name_en,
		'name kh' as name_kh,
		'#,###,##0.00##' as [format], 
		'$' as symbol,
		'1' as show_in_receipt,
		0 as sort_order
		into #tbl_currency
	FROM OPENJSON(@currency, '$')
	WITH  (
			id int '$.currency_id',  
			exchange_rate   decimal(19,10)     '$.exchange_rate',
			change_exchange_rate   decimal(19,10)     '$.change_exchange_rate'
	); 
	 

	select 
		*,
		@total_amount * exchange_rate as total_amount
	from #tbl_currency 
	where 
		show_in_receipt = 1 or exchange_rate=1
	order by sort_order </CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="exchange_rate">
          <DataField>exchange_rate</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="change_exchange_rate">
          <DataField>change_exchange_rate</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="name_en">
          <DataField>name_en</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name_kh">
          <DataField>name_kh</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="format">
          <DataField>format</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="symbol">
          <DataField>symbol</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="show_in_receipt">
          <DataField>show_in_receipt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sort_order">
