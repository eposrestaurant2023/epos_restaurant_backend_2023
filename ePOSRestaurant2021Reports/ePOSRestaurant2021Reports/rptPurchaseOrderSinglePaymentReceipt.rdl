      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	select 
	a.document_number,
	c.business_branch_name_en,
	c.address_en,
	c.phone_1,
	c.phone_2,
	a.total_amount
from tbl_purchase_order a 
inner join tbl_business_branch c on c.id = a.business_branch_id

where
cast(a.id as nvarchar(50)) = @id</CommandText>
      </Query>
