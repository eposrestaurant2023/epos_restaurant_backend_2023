    <DataSet Name="SaleProduct">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@printed_by">
            <Value>=Parameters!printed_by.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customer_id">
            <Value>=Parameters!customer_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@customer_group_id">
            <Value>=Parameters!customer_group_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_category_id">
            <Value>=Parameters!product_category_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@product_group_id">
            <Value>=Parameters!product_group_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@outlet_id">
            <Value>=Parameters!outlet_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select
	coalesce(sp.product_category_en,'Not Set') as product_category_en,
	sum(sp.quantity) as quantity,
	sum(sp.cost) as cost,
	sum(sp.price + sp.total_modifier_amount) as price,
	sum(sp.total_discount_amount) as total_discount_amount,
	sum(sp.sub_total) as sub_total,
	sum(sp.total_amount) as total_amount,
	sum(sp.total_tax_amount) as total_tax_amount,
	sum(sp.profit) as profit
	
from tbl_sale_product sp 
	inner join tbl_sale s on s.id = sp.sale_id 
	inner join	dbo.fn_get_business_branch_by_role(@printed_by) b on b.id = s.business_branch_id
	left join tbl_customer c on c.id = s.customer_id 
where
	s.is_deleted = 0 and 
	sp.is_deleted = 0 and 
	sp.status_id in (1,2) and
	s.working_date between @start_date and @end_date and
	coalesce(cast(s.customer_id as nvarchar(50)),'') = iif(@customer_id = '',coalesce(cast(s.customer_id as nvarchar(50)),'') ,@customer_id) and 
	coalesce(c.customer_group_id,0) = iif(@customer_group_id= 0,coalesce(c.customer_group_id,0),@customer_group_id) and 
	sp.product_category_id = iif(@product_category_id=0,sp.product_category_id,@product_category_id) and 
	sp.product_group_id  = iif(@product_group_id = 0, sp.product_group_id,@product_group_id) and 
	cast(s.outlet_id as nvarchar(50))= iif(@outlet_id = '0', cast(s.outlet_id as nvarchar(50)),@outlet_id) and 
	sp.product_category_id = iif(@product_category_id=0,sp.product_category_id,@product_category_id) and 
	sp.product_group_id = iif(@product_group_id =0,sp.product_group_id ,@product_group_id ) 
group by
	coalesce(sp.product_category_en,'Not Set')
order by 
	coalesce(sp.product_category_en,'Not Set')
 </CommandText>
      </Query>
