    <DataSet Name="ExpenseData">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>=Parameters!start_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>=Parameters!end_date.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@business_branch_id">
            <Value>=Parameters!business_branch_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@expense_category_id">
            <Value>=Parameters!expense_category_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@expense_item_id">
            <Value>=Parameters!expense_item_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	e.reference_number,
	e.business_branch_name,
	e.outlet_name,
	e.station_name,
	e.working_day_number,
	e.cashier_shift_number,
	e.expense_date,
	e.expense_category_name,
	e.expense_item_name,
	e.expense_by,
	e.note,
	e.paymen_type,
	e.amount,
	e.base_currency_amount,
	iif(c.is_prefix_symbol = 1,c.symbol +' '+ c.currency_format, c.currency_format   +' '+ c.symbol ) as currency_format
from tbl_expense e 
inner join tbl_currency c on e.currency_id = c.id
where
	e.is_deleted = 0 and 
	e.expense_date  between @start_date and @end_date and 
	cast(e.business_branch_id as nvarchar(50)) = iif(@business_branch_id='', cast(e.business_branch_id as nvarchar(50)),@business_branch_id)   and
	cast(e.expense_category_id as nvarchar(50)) =  iif(@expense_category_id='', cast(e.expense_category_id as nvarchar(50)),@expense_category_id)    and
	cast(e.expense_item_id as nvarchar(50)) =  iif(@expense_item_id='', cast(e.expense_item_id as nvarchar(50)),@expense_item_id)
order by e.expense_date asc
</CommandText>
      </Query>
