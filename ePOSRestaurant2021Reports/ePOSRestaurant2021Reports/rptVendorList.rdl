    <DataSet Name="Vendor">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@vendor_group_id">
            <Value>=Parameters!vendor_group_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
	company_name,
	vendor_name,
	vendor_code,
	office_phone,
	total_payable,
	b.vendor_group_name_en
from  tbl_vendor a 
	inner join tbl_vendor_group b on a.vendor_group_id = b.id
where a.vendor_group_id = iif(@vendor_group_id = 0,a.vendor_group_id,@vendor_group_id)</CommandText>
      </Query>
