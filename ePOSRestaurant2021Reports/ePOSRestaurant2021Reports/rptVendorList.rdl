    <DataSet Name="VendorGroup">
      <Query>
        <DataSourceName>Datasource</DataSourceName>
        <CommandText>select 0 as id, 'All Vendor Group' as vendor_group_name_en
union
select id,vendor_group_name_en from tbl_vendor_group</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
