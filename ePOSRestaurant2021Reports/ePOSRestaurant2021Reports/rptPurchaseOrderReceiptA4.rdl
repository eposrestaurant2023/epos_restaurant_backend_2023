    <DataSet Name="po_data">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>	select 
	a.document_number,
	c.business_branch_name_en,
	c.address_en,
	c.phone_1,
	c.phone_2,
	v.vendor_code,
	v.vendor_name,
	v.mobile_phone,
	v.office_phone,
    l.stock_location_name,
	a.reference_number,
	a.due_date,
	a.purchase_date,
	a.balance,
	a.created_by,
	a.total_quantity,
	a.sub_total,
	a.total_discount,
	a.discount_type,
	a.discountable_amount,
	a.po_product_discount_amount,
	a.grand_total_discount,
	a.total_amount
from tbl_purchase_order a 
inner join tbl_business_branch c on c.id = a.business_branch_id
inner join tbl_vendor v on v.id = a.vendor_id
inner join tbl_stock_location l on l.id = a.stock_location_id
where
cast(a.id as nvarchar(50)) = @id</CommandText>
      </Query>
