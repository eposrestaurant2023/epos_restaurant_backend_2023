    <DataSet Name="po_product">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
select 
	a.note,
	p.product_name_en,
	a.total_amount,
	a.total_discount,
	a.unit,
	a.cost,
	a.quantity ,
	a.sub_total
from tbl_purchase_order_product a 
inner join tbl_product p on p.id = a.product_id
where 
	cast(a.purchase_order_id as nvarchar(50)) = @id and
	a.is_deleted = 0 and
	a.status = 1</CommandText>
      </Query>
