    <DataSet Name="payment_data">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
p.payment_type_name_en,
p.payment_type_name_kh,
a.currency_format,
sum(a.payment_amount/a.exchange_rate) payment_amount 
from tbl_purchase_order_payment a
inner join tbl_payment_type p on p.id = a.payment_type_id
where purchase_order_id = @id
group by  p.payment_type_name_en,p.payment_type_name_kh , a.currency_format</CommandText>
