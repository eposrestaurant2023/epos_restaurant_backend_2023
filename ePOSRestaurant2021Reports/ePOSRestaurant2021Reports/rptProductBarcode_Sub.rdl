          <QueryParameter Name="@product_category_id">
            <Value>=Parameters!product_category_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
--GET PRODUCT ID ONLY FOR FILTER BY BRANCH

if object_id('tempdb..#tbl_product') is not null drop table #tbl_product;
select 
	distinct product_id 
	into #tbl_product 
from tbl_product_menu pm
	inner join tbl_menu m on m.id = pm.menu_id
	inner join	dbo.fn_get_business_branch_by_role(@printed_by) b on b.id = m.business_branch_id
where 
	cast(m.business_branch_id as nvarchar(50)) = iif(@business_branch_id='', cast(m.business_branch_id as nvarchar(50)),@business_branch_id) 
--GET PRODUCT WITH MORE DETAIL INFORMATION 
  
	 
if object_id('tempdb..#tbl') is not null drop table #tbl
select 
	p.id,
	p.product_category_en, 
	p.product_category_kh,
	p.product_code,
	p.product_name_en,
	p.product_name_kh,
	p.cost,
	p.min_price,
	p.max_price,
	p.unit_id,
	u.unit_name,
	p.quantity,
	p.is_inventory_product
	into #tbl
from tbl_product p 
	inner join #tbl_product bp on bp.product_id = p.id
	inner join tbl_product_category pc on pc.id = p.product_category_id
	inner join tbl_unit u on u.id = p.unit_id 
	
where
	ltrim(rtrim(isnull(p.product_code,''))) &lt;&gt; '' and
	p.is_deleted = 0 and 
	p.status = 1 and 
	p.is_menu_product = 1 and 
	p.product_category_id = iif(@product_category_id = 0, p.product_category_id, @product_category_id)
	 

 

  

 
 



--return product

  ;with a as (
	select 
		ROW_NUMBER() over(order by id )  as column_group , 
		product_category_en,
		product_name_en,
		product_code ,
		min_price
		from #tbl  
)
,b as (
	select  
		(column_group+3)/4 as row_group, 
		product_name_en, 
		product_code,
		min_price,
		product_category_en 
	from a 
)
select 
	row_group, 
	ROW_NUMBER() over(partition by row_group order by row_group) as column_group, 
	product_code,
	product_name_en,
	min_price 
from b
</CommandText>
