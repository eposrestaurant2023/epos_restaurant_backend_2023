<Com is_visible="is_visible">
    @if (!model.is_deleted)
    {
        if (model.is_fulfilled)
        {
            <span class="uk-label fulfilled">Fulfilled</span>
        }else{
            <span class="uk-label draft">Draft</span>
        }
    } 
    <LabelDeleted is_visible="@(model.is_deleted)" />
</Com>

@code{
    [Parameter] public StockTransferModel model { get; set; }
    [Parameter] public bool is_visible { get; set; } = true;
}
