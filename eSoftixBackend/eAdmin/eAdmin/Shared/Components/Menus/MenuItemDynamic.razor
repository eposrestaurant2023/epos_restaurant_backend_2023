@inherits UIComponentBase
<li class="nav-item">
    <NavLink class="nav-link" href="@MenuUrl" Match="@Match">
        <Icon icon="@IconName" className="color_nav" />
        <span class="sidemenu_title">@ChildContent</span>
    </NavLink>
</li>

@code{
    [Parameter] public string IconName { get; set; } = "";
    [Parameter] public string MenuUrl { get; set; } = "";
    [Parameter] public NavLinkMatch Match { get; set; } = NavLinkMatch.Prefix;
}